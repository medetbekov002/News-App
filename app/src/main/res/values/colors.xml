<?xml version="1.0" encoding="utf-8"?>
<resources>
    // news app
    <color name="colorPrimary">#282b33</color>
    <color name="colorPrimaryDark">#282b33</color>
    <color name="colorAccent">#FF9800</color>
    <color name="tittleTextColor">#131214</color>
    <color name="subTittleTextColor">#333233</color>
    <color name="bgLineColor">#606470</color>
    <color name="background">#dddddd</color>
    <color name="card_color">#2e343d</color>
    <color name="white">#00FFFFFF</color>

    <style name="TittleTextStyle">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/tittleTextColor</item>
        <item name="android:fontFamily" />
    </style>

    <style name="SubTittleTextStyle">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/subTittleTextColor</item>
        <item name="android:fontFamily" />
    </style>

    <style name="NormalTextStyle">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/subTittleTextColor</item>
        <item name="android:fontFamily" />
    </style>
</resources>